{
  "name": "reduxstagram",
  "version": "1.0.0",
  "description": "A react, redux, react router tutorial.",
  "main": "main.js (both client/ & server/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/sjm-practice/reduxstagram.git"
  },
  "keywords": [
    "react",
    "redux",
    "react router",
    "jest",
    "flow"
  ],
  "author": "Steve Marsh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sjm-practice/reduxstagram/issues"
  },
  "homepage": "https://github.com/sjm-practice/reduxstagram",
  "scripts": {
    "start": "meteor run",
    "lint": "eslint . --ext .js,.jsx --ignore-pattern 'packages/**'",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow:check": "flow check",
    "flow:status": "flow status",
    "flow:stop": "flow stop",
    "pretest": "yarn run lint && yarn run flow:check",
    "test": "yarn run unit-test",
    "unit-test": "jest -c .jest-config.json",
    "unit-test-w": "yarn run unit-test -- --watchAll"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "meteor-node-stubs": "~0.2.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redbox-react": "^1.3.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "cheerio": "^0.22.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.41.0",
    "jest": "^18.1.0",
    "lodash": "^4.17.4",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.1"
  }
}
